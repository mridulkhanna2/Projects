{
  "name": "Bot",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "This extension was created to automate Intershala functions",
  "homepage_url": "https://github.com/mridulkhanna2",
  "icons": {
    "16": "assets/icons/icon16.png",
    "48": "assets/icons/icon48.png",
    "128": "assets/icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "html/background.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": "assets/icons/icon19.png",
    "default_title": "browser action demo",
    "default_popup": "html/browser_action.html"
  },
  "permissions": [
    "tabs",
    "https://*/*"
  ],
  "content_scripts": [
    
    {
      "matches": [
        "https://internshala.com/"
      ],
      "js": [
        "./function/app.js"
      ]
    }
  ]
}